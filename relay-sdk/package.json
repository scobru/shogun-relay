{
  "name": "shogun-relay-sdk",
  "version": "1.0.1",
  "description": "SDK for interacting with Shogun Relay API - HTTP client for IPFS, deals, registry, and network operations",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist/**/*.js",
    "dist/**/*.d.ts"
  ],
  "scripts": {
    "build": "tsc",
    "prepublishOnly": "npm run build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "shogun",
    "relay",
    "sdk",
    "ipfs",
    "gun",
    "decentralized",
    "storage",
    "api-client"
  ],
  "author": "scobru",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/shogun-network/shogun-2.git",
    "directory": "shogun-relay/relay-sdk"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "form-data": "^4.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
