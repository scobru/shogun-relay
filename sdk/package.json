{
  "name": "shogun-relay-sdk",
  "version": "1.2.10",
  "description": "SDK for interacting with Shogun Relay API - HTTP client for IPFS, deals, registry, and network operations",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist/**/*.js",
    "dist/**/*.d.ts"
  ],
  "scripts": {
    "build": "tsc",
    "prepublishOnly": "npm run build",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint src --max-warnings 0",
    "lint:fix": "eslint src --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\""
  },
  "keywords": [
    "shogun",
    "relay",
    "sdk",
    "ipfs",
    "gun",
    "decentralized",
    "storage",
    "api-client"
  ],
  "author": "scobru",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/shogun-network/shogun-2.git",
    "directory": "shogun-relay/relay-sdk"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "form-data": "^4.0.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.15.0",
    "@types/node": "^20.0.0",
    "eslint": "^9.15.0",
    "globals": "^15.12.0",
    "prettier": "^3.7.4",
    "typescript": "^5.0.0",
    "typescript-eslint": "^8.15.0"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
